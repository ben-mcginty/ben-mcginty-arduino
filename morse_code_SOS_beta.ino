int ledPin = 13;

void setup() {
  pinMode(ledPin, OUTPUT);
}

void loop() {
for(int x=0;x <3;x++){
  digitalWrite (ledPin,HIGH);
  delay (250);
  digitalWrite (ledPin,LOW);
  delay (250);
}
for(int x=0;x <3;x++){
  digitalWrite (ledPin,HIGH);
  delay (1000);
  digitalWrite (ledPin,LOW);
  delay (250;
}
for(int x=0;x <3;x++){
  digitalWrite (ledPin,HIGH);
  delay (250);
  digitalWrite (ledPin,LOW);
  delay (250);
}
delay (2000);
}

